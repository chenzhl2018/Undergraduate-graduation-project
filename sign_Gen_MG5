import model SM_HeavyN_NLO
define p = g u c d s u~ c~ d~ s~
define j = p
generate p p > mu+ mu+ j j QED=4 QCD=0 $$ w+ w- / n2 n3 
add process p p > mu- mu- j j QED=4 QCD=0 $$ w+ w- / n2 n3 
output Launch/sign_Gen
launch
set mN1 scan:[150,1500,5000,15000]
set mN2 1e10
set mN3 1e10
set VeN1 0
set VeN2 0
set VeN3 0
set VmuN1 1
set VmuN2 1
set VmuN3 1
set VtaN1 0
set VtaN2 0
set VtaN3 0
set pdlabel lhapdf
set lhaid 324900
set dynamical_scale_choice 3
set ptj 20
set etaj 5.5
set ptl 10
set etal 4


